{
	"name": "klubiq-db",
	"version": "0.0.1",
	"description": "Klubiq Database",
	"type": "commonjs",
	"devDependencies": {
		"@types/node": "^20.3.1",
		"ts-node": "^10.9.2",
		"typescript": "^5.1.3"
	},
	"dependencies": {
		"@app/common": "file:../libs/common",
		"@automapper/classes": "^8.8.1",
		"@automapper/core": "^8.8.1",
		"class-transformer": "^0.5.1",
		"lodash": "^4.17.21",
		"luxon": "^3.4.4",
		"typeorm": "^0.3.20",
		"@types/lodash": "^4.17.1",
		"@types/luxon": "^3.4.2",
		"dotenv": "^16.4.5",
		"pg": "^8.4.0",
		"reflect-metadata": "^0.1.13"
	},
	"scripts": {
		"build": "npx tsc",
		"start": "ts-node src/index.ts",
		"typeorm": "typeorm-ts-node-commonjs",
		"generate:migration": "typeorm migration:generate build/migrations/changes -o -d build/data-source.js",
		"migration": "typeorm migration:run -d build/data-source.js",
		"migration:revert": "typeorm migration:revert -d build/data-source.js"
	}
}
